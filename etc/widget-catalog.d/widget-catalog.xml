<?xml version="1.0" ?>
<glade-catalog library="gladepython" depends="gtk+" domain="glade-3" name="ElrondWidgets">

  <init-function>glade_python_init</init-function>

  <glade-widget-classes>
    <glade-widget-class generic-name="ElrondExampleWidget" name="ElrondExampleWidget"
                        title="Elrond Example Widget"/>
    <glade-widget-class generic-name="ConsoleWidget" name="ConsoleWidget"
                        title="Console Widget"/>
    <glade-widget-class generic-name="DialogWidget" name="DialogWidget"
                        title="Dialog Widget"/>
    <glade-widget-class generic-name="PlaneWidget" name="PlaneWidget"
                        title="Plane Widget"/>
    <glade-widget-class generic-name="WindowWidget" name="WindowWidget"
                        title="Window Widget"/>
    <glade-widget-class generic-name="YesNoWidget" name="YesNoWidget"
                        title="YesNo Widget"/>
  </glade-widget-classes>

  <glade-widget-group name="Elrond" title="Elrond">
    <glade-widget-class-ref name="ElrondExampleWidget"/>
    <glade-widget-class-ref name="ConsoleWidget"/>
    <glade-widget-class-ref name="DialogWidget"/>
    <glade-widget-class-ref name="PlaneWidget"/>
    <glade-widget-class-ref name="WindowWidget"/>
    <glade-widget-class-ref name="YesNoWidget"/>
  </glade-widget-group>

</glade-catalog>
